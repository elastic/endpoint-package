{
  "index_patterns": [
    "try-ecs-*"
  ],
  "mappings": {
    "_meta": {
      "version": "1.11.0"
    },
    "date_detection": false,
    "dynamic_templates": [
      {
        "strings_as_keyword": {
          "mapping": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "match_mapping_type": "string"
        }
      }
    ],
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "action_id": {
        "path": "request.action_id",
        "type": "alias"
      },
      "agent": {
        "properties": {
          "id": {
            "ignore_above": 1024,
            "type": "keyword"
          }
        }
      },
      "agent_id": {
        "path": "agent.id",
        "type": "alias"
      },
      "completed_at": {
        "path": "response.completed_at",
        "type": "alias"
      },
      "data": {
        "path": "request.data",
        "type": "alias"
      },
      "data_stream": {
        "properties": {
          "dataset": {
            "type": "constant_keyword"
          },
          "namespace": {
            "type": "constant_keyword"
          },
          "type": {
            "type": "constant_keyword"
          }
        }
      },
      "ecs": {
        "properties": {
          "version": {
            "ignore_above": 1024,
            "type": "keyword"
          }
        }
      },
      "event": {
        "properties": {
          "action": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "category": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "created": {
            "type": "date"
          },
          "end": {
            "type": "date"
          },
          "hash": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "id": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "ingested": {
            "type": "date"
          },
          "outcome": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "start": {
            "type": "date"
          },
          "type": {
            "ignore_above": 1024,
            "type": "keyword"
          }
        }
      },
      "response": {
        "properties": {
          "action_id": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "completed_at": {
            "type": "date"
          },
          "data": {
            "properties": {
              "command": {
                "ignore_above": 1024,
                "type": "keyword"
              },
              "comment": {
                "norms": false,
                "type": "text"
              }
            },
            "type": "object"
          },
          "started_at": {
            "type": "date"
          },
          "status": {
            "ignore_above": 1024,
            "type": "keyword"
          }
        }
      },
      "started_at": {
        "path": "response.started_at",
        "type": "alias"
      },
      "status": {
        "path": "response.status",
        "type": "alias"
      }
    }
  },
  "order": 1,
  "settings": {
    "index": {
      "mapping": {
        "total_fields": {
          "limit": 10000
        }
      },
      "refresh_interval": "5s"
    }
  }
}